extends ../layout/layout

block variables
  - var title = 'ChoisBox';
  - var description = 'Search Page';
  - var pageClass = 'search-page';

block content
  section.brand-head.brand-page
    .container
      .title
        .title_wrap
          h1 Kinder
          p.mod Найдено: <span>10</span> конфет
        .title_wrap
          p Сортировка по
          <select class="form-select mod" aria-label="Default select example">
            <option selected>По умолчанию</option>
            <option value="1" title="от дешевых к дорогим">от дешевых к дорогим</option>
            <option value="2" title="от дорогих к дешевым">от дорогих к дешевым</option>
          </select>
      .prod-wrap    
        .prod-card
